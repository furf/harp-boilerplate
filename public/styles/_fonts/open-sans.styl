/**
 * Open Sans Light
 */
@font-face
  font-family 'OpenSans'
  font-style normal
  font-weight 300
  src url('/fonts/Open_Sans/OpenSans-Light.ttf') format('truetype')


/**
 * Open Sans Light Italic
 */
@font-face
  font-family 'OpenSans'
  font-style italic
  font-weight 300
  src url('/fonts/Open_Sans/OpenSans-LightItalic.ttf') format('truetype')


